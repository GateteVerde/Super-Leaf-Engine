    }
    
    //Apply gravity.
    else if (state == 2) {
    
        gravity = grav;
    }
}

//Otherwise, if Mario is wearing the Frog Suit
else if (global.powerup == cs_frog) {

    //Set the gravity.
    gravity = 0
    
